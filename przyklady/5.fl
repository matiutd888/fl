int main() {
    int n = 12;

    // Program obliczający silnię
    int it = 1;
    while(it <= n) {
        it = it * it;
    }
    printString("Factorial = ");
    printInt(it);

    // Program sprawdzający czy liczba jest parzysta
    if (n % 2 == 0) {
        printString("Liczba parzysta");
    }
    else {
        printString("Liczba nieparzysta");
    }

}

void main() {
    n = 12;

    // Program obliczający silnię
    int it = 1;
    while(it <= n) {
        it = it * it;
    }
    print("Factorial = " + toString(it));

    // Program sprawdzający czy liczba jest parzysta
    if (n % 2 == 0) {
        print("Liczba parzysta");
    }
    else {
        print("Liczba nieparzysta");
    }

}